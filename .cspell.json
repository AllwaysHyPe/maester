{
  "import": [
    "@cspell/dict-powershell/cspell-ext.json",
    "@cspell/dict-csharp/cspell-ext.json"
  ],
  "version": "0.2",
  "language": "en",
  "words": [
    "Maester",
    "Docusaurus",
    "Entra",
    "Contoso"
  ],
  "ignoreWords": [
    "maester",
    "eidsca",
    "merill",
    "docusaurus",
    "devcontainers",
    "codespaces"
  ],
  "patterns": [
    {
      "name": "ALL-CAPS-WORDS",
      "pattern": "/\b[A-Z0-9]+\b/g",
      "description": "Any word in ALL CAPS."
    }
  ],
  "ignoreRegExpList": [
    "ALL-CAPS-WORDS",
    "Email",
    "github.com/",
    "@"
  ]
}
